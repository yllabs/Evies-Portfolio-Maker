{
	"name": "interactive-portfolio-builder",
	"private": true,
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build"
	},
	"devDependencies": {
		"@chromatic-com/storybook": "^4.1.0",
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@playwright/test": "^1.49.1",
		"@storybook/addon-a11y": "^9.1.0",
		"@storybook/addon-docs": "^9.1.0",
		"@storybook/addon-svelte-csf": "^5.0.7",
		"@storybook/addon-vitest": "^9.1.0",
		"@storybook/sveltekit": "^9.1.0",
		"@sveltejs/adapter-vercel": "^5.8.1",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.16",
		"@types/node": "^22.17.0",
		"@vitest/browser": "^3.2.3",
		"autoprefixer": "^10.4.21",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-storybook": "^9.1.0",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"playwright": "^1.53.0",
		"postcss": "^8.5.6",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"storybook": "^9.1.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^3.4.0",
		"tsx": "^4.20.3",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^7.0.4",
		"vite-plugin-devtools-json": "^0.2.0",
		"vitest": "^3.2.3",
		"vitest-browser-svelte": "^0.1.0"
	},
	"dependencies": {
		"@inlang/paraglide-js": "^2.2.0",
		"@inlang/paraglide-js-adapter-sveltekit": "^0.6.7",
		"lucide-svelte": "^0.536.0",
		"html2canvas": "^1.4.1",
		"jspdf": "^2.5.1",
		"sortablejs": "^1.15.2"
	}
}
